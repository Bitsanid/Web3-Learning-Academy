# WalletConnect Project ID (get one at https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID="YOUR_PROJECT_ID"

# OnchainKit API Key (get one at https://www.coinbase.com/cloud/products/onchainkit)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=

# MiniKit App ID (get one at https://www.coinbase.com/cloud/products/minikit)
NEXT_PUBLIC_MINIKIT_APP_ID=

# Private key for deploying smart contracts
PRIVATE_KEY=

# Endpoint for the Base network
BASE_RPC_URL=https://mainnet.base.org

# Smart Contract Addresses (after deployment)
NFT_BADGE_CONTRACT_ADDRESS=
STAKING_CONTRACT_ADDRESS=
MARKETPLACE_CONTRACT_ADDRESS=
PAYMENT_TOKEN_ADDRESS=
